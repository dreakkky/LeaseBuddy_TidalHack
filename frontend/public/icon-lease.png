<svg width="160" height="160" viewBox="0 0 160 160" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Background gradient -->
    <linearGradient id="bgGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#4F8CFF"></stop>
      <stop offset="100%" stop-color="#6EE7B7"></stop>
    </linearGradient>

    <!-- Document shadow -->
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="0" dy="3" stdDeviation="3" flood-color="#000" flood-opacity="0.15"></feDropShadow>
    </filter>
  </defs>

  <!-- Background -->
  <circle cx="80" cy="80" r="70" fill="url(#bgGrad)"></circle>

  <!-- Document -->
  <g filter="url(#shadow)">
    <rect x="45" y="35" width="60" height="80" rx="10" fill="white"></rect>
    
    <!-- Folded corner -->
    <polygon points="85,35 105,35 105,55" fill="#E5E7EB"></polygon>
  </g>

  <!-- Lease text lines -->
  <line x1="55" y1="65" x2="95" y2="65" stroke="#9CA3AF" stroke-width="3" stroke-linecap="round"></line>
  <line x1="55" y1="80" x2="95" y2="80" stroke="#9CA3AF" stroke-width="3" stroke-linecap="round"></line>
  <line x1="55" y1="95" x2="80" y2="95" stroke="#9CA3AF" stroke-width="3" stroke-linecap="round"></line>

  <!-- AI neural sparkle -->
  <g fill="none" stroke="white" stroke-width="2">
    <circle cx="115" cy="50" r="4"></circle>
    <circle cx="125" cy="40" r="3"></circle>
    <circle cx="135" cy="50" r="4"></circle>
    <line x1="119" y1="47" x2="122" y2="43"></line>
    <line x1="128" y1="43" x2="131" y2="47"></line>
  </g>
</svg>
